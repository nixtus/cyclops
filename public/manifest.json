{
    "name": "Cyclops",
    "description": "Modify HTTP request headers",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
    },
    "icons": {
        "16": "logo192.png",
        "48": "logo192.png",
        "128": "logo192.png"
    },
    "host_permissions": ["<all_urls>"],
    "permissions": ["activeTab", "storage", "declarativeNetRequest", "downloads"]
}
